{
	"type": "light",
	"colors": {
		"foreground": "#000000",
		"focusBorder": "#000000",
		"widget.shadow": "#868686",
		"input.background": "#f4f4f4",
		"input.foreground": "#000000",
		"input.border": "#000000",
		"inputOption.activeBorder": "#000000",
		"dropdown.background": "#e6e6e6",
		"dropdown.foreground": "#000000",
		"dropdown.border": "#e6e6e6",
		"list.focusBackground": "#a9a9a9",
		"list.activeSelectionBackground": "#a9a9a9",
		"list.inactiveSelectionBackground": "#a9a9a9",
		"list.activeSelectionForeground": "#f4f4f4",
		"list.hoverBackground": "#e6e6e6",
		"list.highlightForeground": "#ff0000",
		"pickerGroup.foreground": "#268bd2",
		"editor.background": "#f4f4f4",
		"editor.foreground": "#000000",
		"editor.selectionBackground": "#d3d3d3",
		"editor.findMatchBackground": "#ffff00",
		"editor.findMatchHighlightBackground": "#c9d8ed",
		"editor.findRangeHighlightBackground": "#c9d0d9",
		"editorWidget.background": "#e6e6e6",
		"editor.lineHighlightBackground": "#eaeaea",
		"editorCursor.foreground": "#000000",
		"editorWhitespace.foreground": "#aaaaaa",
		"editorLineNumber.foreground": "#868686",
		"editorHoverWidget.background": "#f4f4f4",
		"editorGroupHeader.tabsBackground": "#f4f4f4",
		"tab.activeBackground": "#f4f4f4",
		"tab.inactiveBackground": "#e7e7e7",
		"tab.border": "#f4f4f4",
		"tab.activeForeground": "#000000",
		"tab.inactiveForeground": "#888888",
		"panel.background": "#f4f4f4",
		"panelTitle.activeForeground": "#000000",
		"panelTitle.activeBorder": "#e6e6e6",
		"statusBar.foreground": "#888888",
		"statusBar.background": "#f4f4f4",
		"statusBar.noFolderBackground": "#595959",
		"activityBar.background": "#f4f4f4",
		"activityBar.foreground": "#000000",
		"activityBar.dropBackground": "#eaeaea",
		"activityBarBadge.background": "#a3a5b7",
		"sideBar.background": "#f4f4f4",
		"sideBarTitle.foreground": "#888",
		"sideBarSectionHeader.background": "#f4f4f4",
		"notification.background": "#268bd2",
		"statusBar.debuggingBackground": "#595959"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#000000ff",
				"background": "#f4f4f4ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75715E"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#68747c"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#0000FF"
			}
		},
		{
			"name": "Regexp",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "String template - `${string}`",
			"scope": [
				"punctuation.definition.string.template",
				"punctuation.definition.string",
				"punctuation.accessor",
				"punctuation.separator.comma",
				"punctuation.terminator",
				"keyword.operator.rest"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3e4449"
			}
		},
		{
			"name": "Punctuation, but not ':'",
			"scope": [
				"punctuation.accessor",
				"punctuation.separator.comma",
				"punctuation.terminator",
				"keyword.operator.rest"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000000"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid.deprecated",
				"invalid.illegal",
				"invalid"
			],
			"settings": {
				"foreground": "#FF1E00"
			}
		},
		{
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316bcd"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#cd3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		}
	]
}
